<%@ page language="java" contentType="text/html; charset=GBK"
    pageEncoding="GBK"  import="java.util.*,com.gsau.pojo.*, com.gsau.service.*,com.gsau.service.servceImpl.*" %>
    
<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<!--文件头开始-->
<LINK href="css/main.css" rel=stylesheet>
	<%@include file="header.jsp"%>



<!--文件体开始-->

		<table cellspacing=1 cellpadding=3 align=center class=tableBorder2>
		<tr>
		<td height=25 valign=middle>
                  <img src="images/Forum_nav.gif" align="middle">
                  <a href=index.jsp>杰普电子商务门户</a> →
					<img border="0" src="images/dog.gif" width="19" height="18">
					欢迎<font color="red"></font>光临！
                </td>
                </tr>
		</table>
              <br>

		<table cellpadding=3 cellspacing=1 align=center class=tableborder1>
		<tr>
			<td valign=middle align=center height=25 background="images/bg2.gif" width=""><font color="#ffffff"><b>序号</b></font></td>
			<td valign=middle align=center height=25 background="images/bg2.gif" width=""><font color="#ffffff"><b>产品名称</b></font></td>
			<td valign=middle align=center height=25 background="images/bg2.gif" width=""><font color="#ffffff"><b>价格</b></font></td>
			<td valign=middle align=center height=25 background="images/bg2.gif" width=""><font color="#ffffff"><b>操作</b></font></td>
		</tr>
			<%int pageSize=5;
			BookService  bookservice=new BookServiceImpl();
			int pageCount=bookservice.bookCount();
			int pageNow=1;
			String spageNow=request.getParameter("pageNow");
			if(spageNow!=null){
			    pageNow=Integer.parseInt(spageNow);
			}
			ArrayList<Book> book=bookservice.SelectBook(pageSize, pageNow);
			session.setAttribute("book", book);
			%>
              <% int i=1;%>
            <c:forEach items="${book}" var="temp">
            <tr>
			<td class=tablebody2 valign=middle align=center width="">${temp.id }</td>
			<td class=tablebody1 valign=middle width="">&nbsp;&nbsp;<a href="productDetail.jsp?productid=${temp.id}">${temp.name}</a></td>
			<td class=tablebody2 valign=middle align=center width="">${temp.price}</td>
			<td class=tablebody1 valign=middle align=center width=""><a href="addProduct.jsp?productid=${temp.id}">
			<img border="0" src="images/car_new.gif" width="97" height="18"></a> </td>
		</tr>
            </c:forEach> 
            <c:set var="pageNow" value="<%=pageNow %>"></c:set> 
            <c:set var="pageCount" value="<%=pageCount %>"></c:set> 
            <tr ><td rowspan="4" style="font-size:15px;color:blue;align:center">
            <c:if test="${pageNow> 1}" >
	        <a href='index.jsp?pageNow=<%=pageNow-1 %>' style="font-size:15px;color:blue;align:center">上一页</a>
	       </c:if>
	       <c:forEach begin="1" end="<%= pageCount%>" var="j">
	       <a href='index.jsp?pageNow=${j}' style="font-size:15px;color:blue;align:center">${ j}</a>
           </c:forEach>
	       <c:if test="${pageCount>pageNow }">
	       <a href='index.jsp?pageNow=<%=pageNow+1 %>' style="font-size:15px;color:blue;align:center">下一页</a>
	       </c:if>&nbsp;&nbsp;&nbsp;当前页<%= pageNow%>/总页数<%= pageCount%></td><tr/>
		   </table>
		
<!--文件尾开始-->
